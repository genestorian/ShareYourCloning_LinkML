    {
      "name": "restriction_enzymes",
      "definition_uri": "https://w3id.org/genestorian/ShareYourCloning_LinkML/restriction_enzymes",
      "from_schema": "https://w3id.org/genestorian/ShareYourCloning_LinkML",
      "exact_mappings": [
