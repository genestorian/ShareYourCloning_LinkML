                },
                "type": {
                    "description": "The type of the source",
                    "enum": [
                        "RepositoryIdSource"
                    ],
                    "type": "string"
                }
            },
            "required": [
                "repository_name",
                "repository_id",
                "id"
            ],
            "title": "RepositoryIdSource",
            "type": "object"
        },
        "RepositoryName": {
            "description": "",
            "enum": [
                "addgene",
                "genbank"
            ],
            "title": "RepositoryName",
            "type": "string"
        },
        "RestrictionAndLigationSource": {
            "additionalProperties": false,
